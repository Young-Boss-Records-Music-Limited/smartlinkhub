
    
  
  <!-- Main content -->
  <div class="main-content" style="margin-top: -24.1px">
    <!-- Top navbar -->
    <nav class="navbar navbar-top navbar-expand-md navbar-dark" id="navbar-main">
      <div class="container-fluid">
        <!-- Brand -->
        <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block" href="../index.html">Songs Directory</a>
        <!-- User -->
        
      </div>
    </nav>
    <!-- Header -->
    <div class="header bg-gradient-primary pb-8 pt-5 pt-md-7"></div>
    </div>
    <!-- Page content -->
    <div class="container-fluid mt--7">
      <!-- Table -->
      <div class="row">
        <div class="col">
          <div class="card shadow">
            <div class="card-header border-0">
              <h3 class="mb-0">Songs</h3>
            </div>
            <div class="table-responsive">
              <table class="table align-items-center table-flush " id="song_tab">
                <thead class="thead-light">
                  <tr>
                    <th scope="col">File Name</th>
                    <th scope="col">Avatar</th>
                    <th scope="col">Artist</th>
                    <th scope="col">No. of times played</th>
                    <th scope="col">Category</th>
                    <th scope="col">Action</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each artistList}}
                  <tr>
                    <th scope="row">
                      <div class="media align-items-center">
                        <a href="#" class="avatar rounded-circle mr-3">
                          <img alt="Image placeholder" src="../assets/img/theme/bootstrap.jpg">
                        </a>
                        <div class="media-body">
                          <span class="mb-0 text-sm"> {{fileName}}</span>
                        </div>
                      </div>
                    </th>
                    <td>
                      <div class="media-body">
                          <span class="mb-0 text-sm"> {{avatar}}</span>
                        </div>
                    </td>
                    <td>
                        <div class="media-body">
                          <span class="mb-0 text-sm"> {{artist}}</span>
                        </div>
                    </td>
                    <td>
                        <div class="media-body">
                          <span class="mb-0 text-sm"> {{timesPlayed}} </span>
                        </div>
                    </td>
                    <td>
                      <form id='form{{@index}}' onsubmit="xyz( '{{_id}}', pop.checked,fav.checked)">
                        <input type="hidden" id="song-id" name="song-id" value={{_id}}>
                         <label class="checkbox-inline">
                           <input type="checkbox" name="pop" id="pop" value="Popular" onclick="activate()"> Popular
                         </label> &nbsp&nbsp&nbsp
                         <label class="checkbox-inline">
                             <input type="checkbox" name="fav" id="fav" value="Favourite" onclick="activate()" > Favourite
                        </label>
                 </form>
                    </td>
                    <td>
                    <form>
                    <input type="hidden" name="uploadedby" id="uploadedby" value={{_id}} />
                    <button class="btn btn-danger far fa-trash-alt fa-lg" style="background-color:#EC7063" onclick="deleteRow(uploadedby.value)">
                    </button>
                    </form>
                    </td>
                  </tr>
                  
                  {{/each}}
                  <tr>
                    <td></td><td></td><td></td><td></td>
                    <td>
                       <button id="save" class="btn btn-success" style="background-color:rgb(120, 106, 214); border:rgb(120, 106, 214)" onclick="submitCategory()" disabled>Save</button>
                       
                    </td><td></td>
                  </tr>
                </tbody>
              </table>
             
            </div>
            <div class="card-footer py-4">
              <nav aria-label="...">
                <ul class="pagination justify-content-end mb-0">
                  <li class="page-item disabled">
                    <a class="page-link" href="#" tabindex="-1">
                      <i class="fas fa-angle-left"></i>
                      <span class="sr-only">Previous</span>
                    </a>
                  </li>
                  <li class="page-item active">
                    <a class="page-link" href="#">1</a>
                  </li>
                  <li class="page-item">
                    <a class="page-link" href="#">2 <span class="sr-only">(current)</span></a>
                  </li>
                  <li class="page-item"><a class="page-link" href="#">3</a></li>
                  <li class="page-item">
                    <a class="page-link" href="#">
                      <i class="fas fa-angle-right"></i>
                      <span class="sr-only">Next</span>
                    </a>
                  </li>
                </ul>
              </nav>
            </div>
          </div>
        </div>
      </div>

      <!-- Footer -->
      <footer class="footer">
        <div class="row align-items-center justify-content-xl-between">
          <div class="col-xl-6">
            <div class="copyright text-center text-xl-left text-muted">
